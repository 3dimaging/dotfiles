KeepAlive yes
ServerAliveInterval 60
StrictHostKeyChecking no

Host *.bitbucket.org
  User sfarley

Host util01
  User sfarley
  HostName util01.ash-private.bitbucket.org
  CheckHostIP yes
  ProxyCommand none

Host *.ash-private.bitbucket.org *.ash1.bb-inf.net
  User sfarley
  ProxyCommand ssh -A -W %h:%p util01

Host app01
  User sfarley
  HostName app01.ash1.bb-inf.net
  ProxyCommand ssh -A -W %h:%p util01

Host winxp
  HostName localhost
  Port 10722

Host win7
  HostName localhost
  Port 10723

Host euclid
  HostName farley.in
  Port 22537
  ControlMaster auto
  ControlPersist yes
  ControlPath ~/.ssh/socket-%r@%h:%p
  AddressFamily inet

Host *.amazonaws.com
  User ec2-user

Host *
  ForwardAgent yes
